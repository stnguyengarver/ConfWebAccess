@model IEnumerable<ConfWebAccess.Models.studentclass>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            id
        </th>
        <th>
            description
        </th>
        <th>
            starttime
        </th>
        <th>
            duration
        </th>
        <th>
            room
        </th>
        <th>
            scan date
        </th>
        <th>
            ecu
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.lecture.description)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.lecture.starttime)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.lecture.duration)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.lecture.room)
    </td>

    <td>
        @Html.DisplayFor(modelItem => item.scandate)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.lecture.ecu)
    </td>
    <td>
        @Html.ActionLink("Delete", "Delete", new { id = item.id })
    </td>
</tr>
}

</table>
